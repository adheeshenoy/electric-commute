<h5>Other Options</h5>
<table class="table table-hover">
<thead>
    <tr>
    <th scope="col">Electric Transport</th>
    <th scope="col"><%=title%></th>
    </tr>
</thead>
<tbody>
    <%
    for (var key in info) { 
        if (key != transportName){
    %>
    <tr>
        <td><%= key %></td>
        <% if(info[key] == "Out of Range"){ %>
            <td class="bg-danger text-white"><%= info[key] %></td>
        <% } else { %>
            <td><%= info[key] %></td>
        <%}%>
    </tr>
    <% }
}%>
</tbody>
</table>